; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Swords And Witches Multiplayer"
run/main_scene="res://components/game_tree/game_tree.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameMgr="*res://components/game_mgr/game_mgr.gd"
QuestMgr="*res://components/quest_mgmt/quest_mgr.gd"
FactionMgr="*res://components/faction_mgr/faction_mgr.gd"
TileInstancesGlobal="*res://addons/tile-instances/tile_instances_global.gd"
Server="*res://components/multiplayer/server.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"ExampleChar2": "res://dialog/characters/ExampleChar2.dch",
"Travh98": "res://dialog/characters/Travh98.dch"
}
directories/dtl_directory={
"example_dialog": "res://dialog/timelines/example_dialog.dtl"
}

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/tile-instances/plugin.cfg")

